#include<bits/stdc++.h>
using namespace std;
int main()
{
	int x,y,i;
	x=0;
	y=0;
	char s[200];
	gets(s);
	for (i=0;i<strlen(s);i++)
	{
		if(s[i]=='L' || s[i]=='l')
		{
			x=x-1;
		}
		else if(s[i]=='R' || s[i]=='r')
		{
			x=x+1;
		}
		else if(s[i]=='U' || s[i]=='u')
		{
			y=y+1;
		}
		else if(s[i]=='D' || s[i]=='d')
		{
			y=y-1;
		}
	}
	cout<<x<<" "<<y;

}
